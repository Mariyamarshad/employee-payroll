const express = require("express")
const router = express.Router();
const{authMiddleware, verifyAdmin }  = require("../../middlewares/AuthMiddleware")
const { addEmployee, getAllEmployees } = require("../../controllers/adminControllers/AddEmployee")

 
router.post("/add-employee", authMiddleware, verifyAdmin,  addEmployee)
router.get("/get-employees",getAllEmployees)

module.exports = router